extends layout 

block content
  h1= title

  h2= model + ' Series'
    ul
    each guitar in seriesList
      li 
        a(href= guitar.url) #{guitar.series}
    else 
      li The are no guitars

  h2= 'All ' + title
  ul
    each guitar in guitarList
      li 
        a(href=guitar.url) #{guitar.series}
    else 
      li The are no guitars
